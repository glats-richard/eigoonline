# Environment Variables
# Copy this file to .env and fill in your values

# Database connection string (required for campaign management)
DATABASE_URL=postgresql://user:password@host:port/database

# Webhook secret for N8N integration (required)
# Generate a secure random string and use it in both N8N and this app
WEBHOOK_SECRET=your-secret-token-here

# Tracking (existing)
EIGOONLINE_TRACKING_ENABLED=1
